<div id="wizard-frame">
  
  <div id="wizard-header">
    <div class="header-text">
      <h2><%= name %></h2>
    </div>
    
    <div class="wizard-header-right">
      <div class="subtotal">
        <h3>Subtotal: $yy.xx</h3>
      </div>
      
      <div class="add-cart">
        <form action='/orders/populate' method='POST'>
          <button type='submit' value="Add To Cart">Add To Cart</button>
        </form>
      </div>
    </div>
  </div>
  
  <div class="wizard-box">
    <div id="steps"></div>
    <div id="review-share-box"></div></div>
  </div>
</div>


<div id="product-selection">

  <div id="product-container">
    <div id="product-list"></div>
  </div>
</div>

<div id="product-detail-frame"></div>
