<% content_for :page_actions do %>
  <li><%= button_link_to Spree.t(:back_to_wizards_list), session[:return_to] || admin_wizards_url, :icon => 'icon-arrow-left' %></li>
<% end %>

<%= render :partial => 'spree/admin/shared/product_sub_menu' %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @wizard } %>

<p>
  <b>Name:</b>
  <%= @wizard.name %>
</p>

<p>
  <b>Permalink:</b>
  <%= @wizard.permalink %>
</p>


<%= link_to 'Edit', edit_admin_wizard_path(@wizard) %> |
<%= link_to 'Back', admin_wizards_path %>
