<h1>Listing wizards</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @wizards.each do |wizard| %>
    <tr>
      <td><%= wizard.name %></td>
      <td><%= link_to 'Show', wizard %></td>
      <td><%= link_to 'Pre-Selectd Path', wizard_preselected_variants_path(wizard) %></td>
      <td><%= link_to 'Pre-Selectd Url', wizard_preselected_variants_url(wizard) %></td>
    </tr>  
  <% end %>
</table>

<br />

